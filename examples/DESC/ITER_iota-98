N_grid =  0

# solver tolerances
ftol = 1e-4
xtol = 1e-6
gtol = 1e-6
nfev = 250

# solver methods
optimizer         = lsq-exact
objective         = force
spectral_indexing = fringe
node_pattern      = jacobi

# pressure and rotational transform profiles
l:   0	p =   3.4E+6		i =  0.98
l:   2	p =  -3.4E+6

